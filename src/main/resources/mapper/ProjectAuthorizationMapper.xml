<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pighand.user.mapper.project.ProjectAuthorizationMapper">
    <sql id="Base_Column_List">
        `id`,`type`,`matching_env`,`support_logout`,`jwt_salt`,`jwt_body`,`hash_prefix`,`alg`,`expires_in`
    </sql>

    <select id="query" resultType="com.pighand.user.vo.project.ProjectAuthorizationVO">
        select
        <include refid="Base_Column_List"/>
        from project_authorization
    </select>

</mapper>
