<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pighand.user.mapper.user.UserMapper">
    <sql id="Base_Column_List">
        `id`,`project_id`,`password`,`username`,`email`,`email_verified`,`phone_area`,`phone`,`phone_verified`,`initial_source_platform`,
        test
        # ,`status`
    </sql>

    <select id="query" resultType="com.pighand.user.vo.user.UserVO">
        select
        <include refid="Base_Column_List"/>
        from user
    </select>

</mapper>
